{
  "expo": {
    "name": "FixTrackr",
    "slug": "FixTrackr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/FixtrackrLogo.png", 
    "scheme": "FixTrackr",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/LoadingScreen.png",  
      "resizeMode": "cover", 
      "backgroundColor": "#0466c8"  
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/FixtrackrLogo.png",  
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/FixtrackrLogo.png"  
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
